// 9

import java.util.ArrayList;
import java.util.Arrays;

public class SumArrConditional {

	public static void main(String[] args) {

		int[] arr = { 7, 1, 2, 3, 4, 6 };
		ArrayList<Integer> list = new ArrayList<>();

		int totalSum = 0;
		int tempSum = 0;
		boolean count = true;
		boolean found6 = false;

		for (int i = 0; i < arr.length; i++) {

			totalSum += arr[i];

			if (arr[i] == 6) {
				count = false;
				if (!list.contains(6))
					list.add(6);
			}

			if (arr[i] == 7) {
				if (list.contains(6)) {
					count = true;
					list.remove(6);
				}
			}

			if (count == false) {
				tempSum += arr[i];
			}

		}

		if (list.isEmpty()) {
			totalSum -= tempSum;
		}
		System.out.println(totalSum);
	}

}
