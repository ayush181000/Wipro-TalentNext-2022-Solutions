// 1

import java.util.Scanner;

public class ArrayHandling {

	static Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {

		int x;
		x = sc.nextInt();
		int[] nums = new int[x];

		for (int i = 0; i < x; i++) {
			nums[i] = sc.nextInt();
		}

		int pos = sc.nextInt();
		System.out.println(nums[pos+1]);

	}

}
