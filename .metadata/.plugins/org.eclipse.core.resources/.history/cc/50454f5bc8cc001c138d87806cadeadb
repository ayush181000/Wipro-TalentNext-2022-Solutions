import java.util.Arrays;

// 9

public class SumArrConditional {

	public static void main(String[] args) {

		int[] arr = { 97, 2, 5, 7, 89, 43, 12, 67, 9, 56, 23, 6, 234, 7, 678 };
		Arrays.sort(arr);

		int sum = 0;
		boolean count = true;
		for (int i = 0; i < arr.length; i++) {

			if (arr[i] == 6) {
				count = false;
			}

			if (arr[i] == 7) {
				count = true;
			}
			if (count) {
				sum = sum + arr[i];
			}
		}
	}

}
