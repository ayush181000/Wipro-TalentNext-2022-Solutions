// 13

import java.util.Scanner;

public class ReverseMatrix {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int[][] mat = new int[2][2];

		// input
		for (int i = 0; i < 2; i++) {
			for (int j = 0; j < 2; j++) {
				mat[i][j] = sc.nextInt();
			}
		}

		// swap
		for (int i = 0; i < 1; i++) {
			for (int j = 0; j < 1; j++) {
				int temp = mat[i][j];
				mat[i][j] = mat[2 - j][2 - i];
				mat[2 - j][2 - i] = temp;
			}
		}

		// print
		for (int i = 0; i < 2; i++) {
			for (int j = 0; j < 2; j++) {
				System.out.print(mat[i][j] + " ");
			}
			System.out.println();
		}

	}

}
