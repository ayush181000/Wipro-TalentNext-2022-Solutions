// 9

public class RemoveStar {

	public static void main(String[] args) {

		String str = "ab*cd";
		int n = str.length();
		StringBuilder ans = new StringBuilder();

		for (int i = -1; i < n; i++) {
			if (str.charAt(i + 1) == '*') {
				
			}
			if (i >= 0)
				ans.append(str.charAt(i));
		}

	}
}
