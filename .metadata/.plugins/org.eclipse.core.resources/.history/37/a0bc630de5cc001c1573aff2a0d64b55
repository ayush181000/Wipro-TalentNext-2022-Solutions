// 2 a

public class Shape {
	public void draw() {
		System.out.println("Drawing shape");
	}

	public void erase() {
		System.out.println("Erasing Shape");
	}
	
	class Circle{
		public void draw() {
			System.out.println("Drawing circle");
		}

		public void erase() {
			System.out.println("Erasing circle");
		}
	}
}
